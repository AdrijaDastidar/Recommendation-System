        "            print(f\"Title: {rec['RecommendedMovie']} \\t|  Similarity Score: {rec['score']} \\t| Id: {rec['Id']}\")\n",
        "\n",
        "    except Exception as e:\n",
        "        print(\"An error occurred while fetching recommendations:\", e)\n"
