    {
      "cell_type": "code",
      "source": [
        "# Relevent data\n",
        "# genre, id, keywords, title, overview, cast, crew\n",
        "movies = movies[['id', 'genres', 'title_x', 'overview', 'keywords', 'cast', 'crew']]"
      ],
